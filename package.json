{
    "name": "vue-ts",
    "version": "0.0.0",
    "node": "16.19.0",
    "private": true,
    "scripts": {
        "dev": "vite dev --port=3000",
        "build": "vite build",
        "preview": "vite preview",
        "test:unit": "vitest --environment jsdom --root src/",
