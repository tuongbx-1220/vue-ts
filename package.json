        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/**/*.{ts,vue}"
    },
    "dependencies": {
        "@vue-js-cron/light": "^2.2.2",
        "axios": "^1.2.2",
        "date-fns": "^2.29.3",
