        "lodash": "^4.17.21",
        "pinia": "^2.0.28",
        "socket.io-client": "^4.6.1",
        "vee-validate": "^4.7.3",
        "vue": "^3.2.45",
        "vue-confetti": "^2.3.0",
        "vue-router": "^4.1.6",
        "vue-select": "^4.0.0-beta.6",
        "vue-socket.io": "^3.0.10",
        "vue-toastification": "^2.0.0-rc.5",
        "vue3-google-login": "^2.0.12",
        "yup": "^0.32.11"
