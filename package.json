        "@vue/test-utils": "^2.2.6",
        "@vue/tsconfig": "^0.1.3",
        "autoprefixer": "^10.4.13",
        "eslint": "^8.22.0",
        "eslint-plugin-vue": "^9.3.0",
        "jsdom": "^20.0.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.20",
        "prettier": "^2.7.1",
        "tailwindcss": "^3.2.4",
        "typescript": "~4.7.4",
